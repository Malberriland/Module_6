class Vehicle:
    vehicle_type = "none"


class Car:
    price = 1000000

    def __init__(self):
        self.ls = 0

    def horse_powers(self):
        print("Л.с.:", self.ls)


class Nissan(Car, Vehicle):
    price = 1200000
    vehicle_type = "Nissan"

    def horse_powers(self):
        self.ls = 200
        print("Л.с.:", self.ls)


nissan = Nissan()
nissan.horse_powers()
print('Тип транспорта:', Nissan.vehicle_type)
print('Цена:', Nissan.price)
