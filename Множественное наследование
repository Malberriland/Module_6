class Vehicle:
    vehicle_type = "none"


class Car:
    price = 1000000

    def __init__(self, ls):
        self.ls = ls

    def horse_powers(self):
        print("Л.с.:", self.ls)


class Nissan(Car, Vehicle):
    price = 1200000
    vehicle_type = "Nissan"

    def __init__(self, ls):
        super().__init__(ls)
        super().horse_powers()


nissan = Nissan(200)
print('Тип транспорта:', Nissan.vehicle_type)
print('Цена:', Nissan.price)
